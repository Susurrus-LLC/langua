{
  "name": "langua",
  "version": "0.4.2",
  "license": "AGPL-3.0",
  "author": "Ian A. Cook",
  "description": "A suite of language tools",
  "repository": {
    "type": "git",
    "url": "https://github.com/Susurrus-LLC/langua"
  },
  "bugs": {
    "url": "https://github.com/Susurrus-LLC/langua/issues"
  },
  "homepage": "https://susurrus-llc.github.io/langua/",
  "private": true,
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "develop": "gatsby develop",
    "format": "prettier-standard --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lintjs": "eslint src/**/*.js",
    "lintmd": "remark .",
    "test": "jest --watch",
    "test-once": "jest",
    "test-with-coverage": "jest --coverage && codacy-coverage < ./coverage/lcov.info"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier-standard"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.1",
    "classnames": "^2.2.6",
    "file-saver": "^2.0.2",
    "gatsby": "^2.24.83",
    "gatsby-image": "^2.4.21",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-manifest": "^2.4.35",
    "gatsby-plugin-offline": "^3.2.36",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "gatsby-plugin-sass": "^2.3.20",
    "gatsby-plugin-sharp": "^2.6.43",
    "gatsby-source-filesystem": "^2.3.35",
    "gatsby-transformer-sharp": "^2.5.19",
    "js-schema": "^1.0.1",
    "node-sass": "^4.14.1",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-helmet": "^6.1.0",
    "react-toastify": "^6.0.9",
    "react-vis": "^1.11.7"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.0",
    "babel-preset-gatsby": "^0.5.14",
    "codacy-coverage": "^3.4.0",
    "eslint": "^7.11.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-standard": "^4.0.2",
    "husky": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.0",
    "lint-staged": "^10.4.2",
    "prettier": "2.1.2",
    "prettier-standard": "^16.4.1",
    "react-test-renderer": "^16.14.0",
    "remark-cli": "^9.0.0",
    "remark-lint": "^8.0.0",
    "remark-preset-lint-recommended": "^5.0.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 9",
    "not op_mini all"
  ]
}
